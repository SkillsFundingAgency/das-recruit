@model DisplayVacancyViewModel
@{
    Layout = LayoutNames.LayoutWithoutNavigation;
}
<div class="grid-row">
    <div class="column-full">
        <h1 class="heading-xlarge" id="vacancy-header">
            @Model.Title
            <span class="heading-secondary">@Model.EmployerName</span>
        </h1>
    </div>
</div>

<partial name="@PartialNames.NonEditableVacancyDisplay" model="Model" />